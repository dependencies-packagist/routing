{
    "name": "annotation/routing",
    "description": "Use PHP 8 attributes to register routes in a Laravel Application.",
    "keywords": [
        "route",
        "routing",
        "laravel",
        "annotation",
        "attribute"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist/routing",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "annotation/route": "^2.0",
        "reflective/reflection": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Annotation\\Routing\\": "src/"
        }
    },
    "provide": {
        "annotation/route-implementation": "2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Annotation\\Routing\\RouteServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
